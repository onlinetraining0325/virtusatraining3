<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="studentOne" class="com.java.spr.Student">
  <property name="sno" value="1"/>
  <property name="name" value="Shaili"/>
  <property name="lunchBox">
    <bean class="com.java.spr.LunchBox">
      <property name="snacks" value="French Fries..."/>
    </bean>
  </property>
</bean>
<bean id="studentTwo" class="com.java.spr.Student">
<property name="sno" value="2"/>
<property name="name" value="Lokesh"/>
<property name="lunchBox">
  <bean class="com.java.spr.LunchBox">
    <property name="snacks" value="Pizza..."/>
  </bean>
</property>
</bean>
<bean id="studentThree" class="com.java.spr.Student">
<property name="sno" value="3"/>
<property name="name" value="Anusri"/>
<property name="lunchBox">
  <bean class="com.java.spr.LunchBox">
    <property name="snacks" value="Burger..."/>
  </bean>
</property>
</bean>
<bean id="beanFaculty" class="com.java.spr.Faculty">
<property name="facultyName" value="Prasanna P"/>
<property name="studentsList">
  <list>
    <ref bean="studentOne"/>
    <ref bean="studentTwo"/>
    <ref bean="studentThree"/>
  </list>
</property>
</bean>
</beans>
